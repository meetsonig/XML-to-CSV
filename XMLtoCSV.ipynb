   "source": [
    "import xml.etree.ElementTree as ET\n",
    "from sys import argv, exit\n",
    "import pandas as pd"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 181,
   "metadata": {},
   "outputs": [],
   "source": [
    "with open('physsim-network.xml', 'r') as xmlFile:\n",
    "    xmlTree = ET.parse(xmlFile)\n",
    "\n",
 
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "\n",
    "\n",
    "# Final"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 182,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_id=pd.DataFrame(columns=['id','x','y'])\n",
    "\n",
    "df_att=pd.DataFrame(columns=['id',\n",
    "               'from',\n",
    "               'to',\n",
    "               'length',\n",
    "               'freespeed',\n",
    "               'capacity',\n",
    "               'permlanes',\n",
    "               'oneway','modes','origid','type'  ])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 183,
   "metadata": {},
   "outputs": [],
   "source": [
    "for treeProperties in xmlTree.findall('.//'):\n",
    "    for prop in treeProperties.findall('node'):\n",
    "        propid = prop.get('id')\n",
    "        propx = prop.get('x')\n",
    "        propy = prop.get('y')\n",
    "        df1=pd.DataFrame([[propid, propx,propy]], columns=['id','x','y'])\n",
    "        #df1=pd.DataFrame({'id':propid,'x':propx,'y':propy})\n",
    "        df_id=df_id.append(df1,ignore_index=True)\n",
    "    \n",
    "    for prop in treeProperties.findall('link'):\n",
    "        lis=[]\n",
    "        for att in prop.findall('attributes/attribute'):\n",
    "            lis.append(att.text)\n",
    "        #propid = prop.get('id')\n",
    "        #propx = prop.get('x')\n",
    "        #propy = prop.get('y')\n",
    "        if not(lis):\n",
    "            df1=pd.DataFrame([[prop.get('id'), prop.get('from'),prop.get('to'),prop.get('length'),prop.get('freespeed'),\n",
    "                           prop.get('capacity'),prop.get('permlanes'),1,prop.get('modes'),'NA','NA']], columns=['id',\n",
    "               'from',\n",
    "               'to',\n",
    "               'length',\n",
    "               'freespeed',\n",
    "               'capacity',\n",
    "               'permlanes',\n",
    "               'oneway','modes','origid','type' ])\n",
    "        else:\n",
    "            df1=pd.DataFrame([[prop.get('id'), prop.get('from'),prop.get('to'),prop.get('length'),prop.get('freespeed'),\n",
    "                           prop.get('capacity'),prop.get('permlanes'),1,prop.get('modes'), lis[0],lis[1]]], columns=['id',\n",
    "               'from',\n",
    "               'to',\n",
    "               'length',\n",
    "               'freespeed',\n",
    "               'capacity',\n",
    "               'permlanes',\n",
    "               'oneway','modes','origid','type'])\n",
    "            \n",
    "        df_att=df_att.append(df1,ignore_index=True)"
   ]
  },
